      text-align: center;
    }
    .filters {
      display: flex;
      gap: 1rem;
      padding: 1rem;
      justify-content: center;
      background: #eee;
    }
    .products {
      display: grid;
      grid-template-columns: repeat(auto-fill, minmax(200px, 1fr));
      gap: 1rem;
      padding: 1rem;
    }
    .product {
      background: #fff;
      padding: 1rem;
      border-radius: 8px;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
      text-align: center;
    }
    .product img {
      max-width: 100%;
      height: 150px;
      object-fit: cover;
      border-radius: 5px;
    }
    .cart, .order-status {
      background: #fff;
      margin: 1rem;
