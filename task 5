<body>
  <header>
    <h1>My E-Commerce Store</h1>
  </header>
  <div class="filters">
    <select id="categoryFilter">
      <option value="all">All</option>
      <option value="electronics">Electronics</option>
      <option value="books">Books</option>
      <option value="fashion">Fashion</option>
    </select>
    <select id="sortFilter">
      <option value="default">Sort By</option>
      <option value="priceLow">Price: Low to High</option>
      <option value="priceHigh">Price: High to Low</option>
    </select>
  </div>
  <div class="products" id="productList"></div>
  <div class="cart">
    <h2>Cart</h2>
    <ul id="cartItems"></ul>
  </div>
  <div class="order-status" id="orderStatus"></div>

  <script>
    const products = [
      { name: "Wireless Headphones", category: "electronics", price: 1200, image: "https://m.media-amazon.com/images/I/51FNnHjzhQL._UF1000,1000_QL80_.jpg", rating: 4 },
      { name: "LED Monitor", category: "electronics", price: 6000, image: "https://m.media-amazon.com/images/I/81Zt42ioCgL._AC_UY218_.jpg", rating: 5 },
      { name: "Bluetooth Speaker", category: "electronics", price: 1500, image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR9cYWSaBNIhOPF2gguIun1olGtIYrHAlOO7w&s", rating: 4 },
      { name: "The Psychology of Money", category: "books", price: 300, image: "https://miro.medium.com/v2/resize:fit:1400/0*iWd_c6rFiGeJJTU0", rating: 5 },
      { name: "Atomic Habits", category: "books", price: 250, image: "https://m.media-amazon.com/images/I/91bYsX41DVL._AC_UY218_.jpg", rating: 4 },
      { name: "Think and Grow Rich", category: "books", price: 220, image: "https://m.media-amazon.com/images/I/81t2CVWEsUL._AC_UY218_.jpg", rating: 4 },
      { name: "Men's T-Shirt", category: "fashion", price: 400, image: "https://m.media-amazon.com/images/I/71g40mlbinL._AC_UL320_.jpg", rating: 4 },
      { name: "Women's Dress", category: "fashion", price: 800, image: "https://www.whitewhale.co.in/cdn/shop/products/WhatsApp_Image_2022-12-14_at_13.43.15_1.jpg?v=1671014587", rating: 5 },
      { name: "Sneakers", category: "fashion", price: 1200, image: "https://images.puma.com/image/upload/f_auto,q_auto,b_rgb:fafafa,w_600,h_600/global/395829/01/sv05/fnd/IND/fmt/png/Carina-Slim-Perf-Women's-Sneakers", rating: 4 },
      { name: "Smartphone", category: "electronics", price: 10000, image: "https://m.media-amazon.com/images/I/71SVRdsgY-L._UF1000,1000_QL80_.jpg", rating: 5 },
      { name: "Gaming Mouse", category: "electronics", price: 700, image: "https://m.media-amazon.com/images/I/61LtuGzXeaL._AC_UY218_.jpg", rating: 4 },
      { name: "Laptop", category: "electronics", price: 35000, image: "https://m.media-amazon.com/images/I/61qlqvTsocL._UF1000,1000_QL80_.jpg", rating: 5 },
      { name: "Notebook Set", category: "books", price: 180, image: "https://images-static.nykaa.com/media/catalog/product/e/9/e95dacf8904345111136_1.jpg?tr=w-500", rating: 3 },
      { name: "Casual Shirt", category: "fashion", price: 600, image: "https://thesparkshop.in/wp-content/uploads/2023/04/Custom-Fashion-Men-prime-S-Hawaiian-Shirt-Casual-Printed-Beach-Summer-Button-Down-Shirts.webp", rating: 4 },
      { name: "Saree", category: "fashion", price: 1500, image: "https://static.cilory.com/783838-thickbox_default/light-green-silk-stone--beads-designer-saree.jpg.webp", rating: 5 },
      { name: "Jeans", category: "fashion", price: 1000, image: "https://www.ruff.in/cdn/shop/files/14744_RE_STONE_1_5a6667b5-3232-4970-9a2f-9335dbe54ecf.jpg?v=1739353119&width=1000", rating: 4 },
      { name: "Cookbook", category: "books", price: 350, image: "data:image/jpeg;base64,", rating: 4 },
      { name: "Tablet", category: "electronics", price: 8000, image: "https://p3-ofp.static.pub/fes/cms/2023/02/22/5rhddw9d8vm9xfkexcwzvvi9lindg8985574.png", rating: 4 },
      { name: "Router", category: "electronics", price: 1100, image: "data:image/jpeg;base64,", rating: 3 },
      { name: "Backpack", category: "fashion", price: 750, image: "https://m.media-amazon.com/images/I/81N7FmJhbhL._AC_UL320_.jpg", rating: 4 }
    ];

    const productList = document.getElementById("productList");
    const cartItems = document.getElementById("cartItems");
    const orderStatus = document.getElementById("orderStatus");

    function displayProducts(list) {
      productList.innerHTML = "";
      list.forEach((p, index) => {
        const card = document.createElement("div");
        card.className = "product";
        card.innerHTML = `
          <img src="${p.image}" alt="${p.name}" loading="lazy">
          <h3>${p.name}</h3>
          <p>₹${p.price}</p>
          <div class="rating">${"★".repeat(p.rating)}${"☆".repeat(5 - p.rating)}</div>
          <button onclick="addToCart(${index})">Add to Cart</button>
          <button onclick="orderNow('${p.name}')">Order</button>
        `;
        productList.appendChild(card);
      });
    }

    function addToCart(index) {
      const p = products[index];
      const li = document.createElement("li");
      li.innerHTML = `<img src="${p.image}" width="40"> ${p.name} - ₹${p.price}`;
      cartItems.appendChild(li);
    }

    function orderNow(name) {
      orderStatus.innerHTML = `<h3>Your order for <em>${name}</em> is confirmed! ✅</h3>`;
    }

    document.getElementById("categoryFilter").addEventListener("change", e => {
      const val = e.target.value;
      if (val === "all") displayProducts(products);
      else displayProducts(products.filter(p => p.category === val));
    });

    document.getElementById("sortFilter").addEventListener("change", e => {
      const val = e.target.value;
      let sorted = [...products];
      if (val === "priceLow") sorted.sort((a, b) => a.price - b.price);
      if (val === "priceHigh") sorted.sort((a, b) => b.price - a.price);
      displayProducts(sorted);
    });

    displayProducts(products);
  </script>
</body>
</html>
