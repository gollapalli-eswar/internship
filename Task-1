      margin: 0;
      padding: 20px;
    }

    header {
      background-color: #4CAF50;
      padding: 10px;
      color: white;
      text-align: center;
      border-radius: 8px;
    }

    main {
      margin-top: 20px;
    }

    img {
      max-width: 100%;
      height: auto;
      border-radius: 8px;
    }

    a {
      color: #0077cc;
      text-decoration: none;
    }

    a:hover {
      text-decoration: underline;
    }

