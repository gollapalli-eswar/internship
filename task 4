<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  <title>Product Store - Filter & Sort</title>
  <style>
    * { box-sizing: border-box; margin: 0; padding: 0; }
    body {
      font-family: 'Segoe UI', sans-serif;
      background: #f7f8fa;
      color: #333;
      padding: 20px;
    }
    h1 {
      text-align: center;
      margin-bottom: 30px;
      font-size: 2.5rem;
      color: #222;
    }
    .controls {
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
      margin-bottom: 20px;
      background: #fff;
      padding: 15px;
      border-radius: 10px;
      box-shadow: 0 2px 6px rgba(0,0,0,0.1);
      gap: 1rem;
    }
    .controls select {
      padding: 8px 12px;
      border: 1px solid #ccc;
      border-radius: 6px;
    }
    .product-grid {
      display: grid;
      grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
      gap: 20px;
    }
    .product {
      background: white;
      border-radius: 10px;
      overflow: hidden;
      box-shadow: 0 4px 10px rgba(0,0,0,0.1);
      text-align: center;
      transition: transform 0.3s ease;
    }
    .product:hover {
      transform: translateY(-5px);
    }
    .product img {
      width: 100%;
      height: 180px;
      object-fit: cover;
    }
    .product h3 {
      font-size: 1.1rem;
      margin: 10px 0 5px;
    }
    .price {
      color: #28a745;
      font-weight: bold;
    }
    .rating {
      color: #ff9800;
      font-size: 0.9rem;
    }
    .category {
      font-size: 0.85rem;
      color: #777;
      margin-bottom: 10px;
    }
  </style>
</head>
<body>
  <h1>üõí Featured Products</h1>

  <div class="controls">
    <div>
      <label>Category: </label>
      <select id="category-filter">
        <option value="all">All</option>
        <option value="electronics">Electronics</option>
        <option value="clothing">Clothing</option>
        <option value="books">Books</option>
      </select>
    </div>
    <div>
      <label>Price: </label>
      <select id="price-filter">
        <option value="all">All</option>
        <option value="0-499">Under ‚Çπ500</option>
        <option value="500-999">‚Çπ500 - ‚Çπ999</option>
        <option value="1000-10000">Above ‚Çπ1000</option>
      </select>
    </div>
    <div>
      <label>Sort by Rating: </label>
      <select id="sort-rating">
        <option value="none">None</option>
        <option value="high">High to Low</option>
        <option value="low">Low to High</option>
      </select>
    </div>
  </div>

  <div class="product-grid" id="product-list"></div>

  <script>
    const products = [
      {
        name: "Wireless Headphones",
        category: "electronics",
        price: 899,
        rating: 4.7,
        image: "https://encrypted-tbn0.gstatic.com/shopping?q=tbn:ANd9GcQByYuVQ2Mb6ikc12XQBv6CpBCtIozaPVKQBBAtnSR57lnNp8nRWH-o0PYEPJCQFMe2bPeNEayDB-qdCLcO6tJo-aHpnmUPEGKth3YoCeM"
      },
      {
        name: "Smartphone 5G",
        category: "electronics",
        price: 9999,
        rating: 4.6,
        image: "https://images.unsplash.com/photo-1511707171634-5f897ff02aa9?auto=format&fit=crop&w=600&q=80"
      },
      {
        name: "Men's Cotton T-Shirt",
        category: "clothing",
        price: 399,
        rating: 4.2,
        image: "data:image/webp;base64,"
      },
      {
        name: "Denim Jeans",
        category: "clothing",
        price: 799,
        rating: 4.0,
        image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQzNRfGZK2CoPGaGVAI5zve5I8Cq_o1W2rTyQ&s"
      },
      {
        name: "Science Textbook",
        category: "books",
        price: 299,
        rating: 4.3,
        image: "data:image/jpeg;base64,"
      },
      {
        name: "Fictional Novel",
        category: "books",
        price: 499,
        rating: 4.8,
        image: "data:image/jpeg;base64,"
      },
      {
        name: "Bluetooth Speaker",
        category: "electronics",
        price: 1099,
        rating: 4.4,
        image: "https://encrypted-tbn3.gstatic.com/shopping?q=tbn:ANd9GcSMT6IbXaC09cOtoKsAMXqt8ozpPQyC9DNPj2VVfkYXoW4b5rMvEPGSLrwtC4VuLofj7-gUs5WZinQubCeF1-lvLM9gitae9rfoIy0Ty_k"
      },
      {
        name: "Casual Sneakers",
        category: "clothing",
        price: 1199,
        rating: 4.5,
        image:"https://encrypted-tbn3.gstatic.com/shopping?q=tbn:ANd9GcRsNuWInrwKhrl0IQ9k613NAvkOYCMH_HkTGJbx17Yd2ghckqUc2NfL9VQQnawGMLIZiE9zjYW7bYawBORy471KC5-0wgnXwTffrPB9j5pWDP0uEngRXaDU"
      }
    ]; 

    const categoryFilter = document.getElementById("category-filter");
    const priceFilter = document.getElementById("price-filter");
    const sortRating = document.getElementById("sort-rating");
    const productList = document.getElementById("product-list");

    categoryFilter.addEventListener("change", filterAndRender);
    priceFilter.addEventListener("change", filterAndRender);
    sortRating.addEventListener("change", filterAndRender);

    function filterAndRender() {
      const cat = categoryFilter.value;
      const price = priceFilter.value;
      const sort = sortRating.value;

      let filtered = [...products];

      if (cat !== "all") {
        filtered = filtered.filter(p => p.category === cat);
      }

      if (price !== "all") {
        const [min, max] = price.split("-").map(Number);
        filtered = filtered.filter(p => p.price >= min && p.price <= max);
      }

      if (sort === "high") filtered.sort((a, b) => b.rating - a.rating);
      if (sort === "low") filtered.sort((a, b) => a.rating - b.rating);

      renderProducts(filtered);
    }

    function renderProducts(list) {
      productList.innerHTML = "";

      list.forEach(product => {
        const div = document.createElement("div");
        div.className = "product";
        div.innerHTML = `
          <img src="${product.image}" alt="${product.name}" />
          <h3>${product.name}</h3>
          <p class="price">‚Çπ${product.price}</p>
          <p class="rating">‚≠ê ${product.rating}</p>
          <p class="category">${capitalize(product.category)}</p>
        `;
        productList.appendChild(div);
      });
    }

    function capitalize(word) {
      return word.charAt(0).toUpperCase() + word.slice(1);
    }

    filterAndRender();
  </script>
</body>
</html>
